package org.example;

class Singleton{
	private int number;
	public Singleton() {
		System.out.println("Inside ctor");
		this.number = -1;
	}
	public int getNumber() {
		return number;
	}
	public void setNumber(int number) {
		this.number = number;
	}
}
public class Program {
	public static void main(String[] args) {
		Singleton s1 = new Singleton();
		Singleton s2 = new Singleton();
	}
}
