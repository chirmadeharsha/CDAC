int menu_list( void ){
	int choice;
	printf("0.Exit\n");
	printf("1.Sum\n");
	printf("2.Sub\n");
	printf("3.Multiplication\n");
	printf("4.Division\n");
	printf("Enter choice	:	");
	scanf("%d", &choice);
	return choice;
}
