package org.example.demo1;

class Person{
	private String name;
	private int age;
	public Person( ) {
		this.name = "";
		this.age = 0;
	}
	public Person(String name, int age ) {
		this.name = name;
		this.age = age;
	}
}
class Employee extends Person{
	
}
public class Program {
	public static void main(String[] args) {

	}
}
