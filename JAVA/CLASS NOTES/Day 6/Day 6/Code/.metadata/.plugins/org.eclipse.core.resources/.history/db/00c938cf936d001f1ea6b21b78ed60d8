package org.example;

class Employee {
    private String name;
    private String empId;
    private String department;
    private String designation;
    private double salary;

    public Employee() {
    	this( "", 0, "", "", 0.0f );
	}
    public Employee(String name, String empId, String department, String designation, double salary) {
        this.name = name;
        this.empId = empId;
        this.department = department;
        this.designation = designation;
        this.salary = salary;
    }

    @Override
    public String toString() {
        return String.format("Employee{name='%s', empId='%s', department='%s', designation='%s', salary=%.2f}", 
                             name, empId, department, designation, salary);
    }

    public static void main(String[] args) {
        Employee emp = new Employee("Alice", "E123", "Finance", "Manager", 75000.00);
        System.out.println(emp.toString()); // Outputs: Employee{name='Alice', empId='E123', department='Finance', designation='Manager', salary=75000.00}
    }
}


public class Program {
	public static void main(String[] args) {
		Object
	}
}
