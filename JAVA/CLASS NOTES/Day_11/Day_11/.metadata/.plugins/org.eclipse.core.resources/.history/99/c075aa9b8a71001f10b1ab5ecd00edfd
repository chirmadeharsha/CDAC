package org.example.main;

import java.util.Scanner;

public class Program {
	public static void main(String[] args) {
		try {
			Scanner sc = null;
			System.out.print("Enter number	:	");
			int number = sc.nextInt();
			System.out.println("Number	:	"+number);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
